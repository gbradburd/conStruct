	file = {PubMed Central Full Text PDF:/Users/gcoop79/Library/Application Support/Firefox/Profiles/0t235o0e.default/zotero/storage/R28TBZS7/Moorjani et al. - 2013 - Genetic Evidence for Recent Population Mixture in .pdf:application/pdf}
