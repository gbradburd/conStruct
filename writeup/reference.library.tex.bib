@article{lipson_mixmapper_2013,
	title = {Efficient Moment-Based Inference of Admixture Parameters and Sources of Gene Flow},
	volume = {30},
	OPTissn = {0737-4038},
	OPTurl = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3708505/},
	OPTdoi = {10.1093/molbev/mst099},
	abstract = {The recent explosion in available genetic data has led to significant advances in understanding the demographic histories of and relationships among human populations. It is still a challenge, however, to infer reliable parameter values for complicated models involving many populations. Here, we present {MixMapper}, an efficient, interactive method for constructing phylogenetic trees including admixture events using single nucleotide polymorphism ({SNP}) genotype data. {MixMapper} implements a novel two-phase approach to admixture inference using moment statistics, first building an unadmixed scaffold tree and then adding admixed populations by solving systems of equations that express allele frequency divergences in terms of mixture parameters. Importantly, all features of the model, including topology, sources of gene flow, branch lengths, and mixture proportions, are optimized automatically from the data and include estimates of statistical uncertainty. {MixMapper} also uses a new method to express branch lengths in easily interpretable drift units. We apply {MixMapper} to recently published data for Human Genome Diversity Cell Line Panel individuals genotyped on a {SNP} array designed especially for use in population genetics studies, obtaining confident results for 30 populations, 20 of them admixed. Notably, we confirm a signal of ancient admixture in European populationsâ€”including previously undetected admixture in Sardinians and Basquesâ€”involving a proportion of 20â€“40\% ancient northern Eurasian ancestry.},
	number = {8},
	urldate = {2014-12-31},
	journal = {Molecular Biology and Evolution},
	author = {Lipson, Mark and Loh, Po-Ru and Levin, Alex and Reich, David and Patterson, Nick and Berger, Bonnie},
	month = aug,
	year = {2013},
	pmid = {23709261},
	pmcid = {PMC3708505},
	pages = {1788--1802},
	file = {PubMed Central Full Text PDF:/Users/gcoop79/Library/Application Support/Firefox/Profiles/0t235o0e.default/zotero/storage/FH7TKSWI/Lipson et al. - 2013 - Efficient Moment-Based Inference of Admixture Para.pdf:application/pdf}
}


@article{moorjani_india_2013,
	title = {Genetic evidence for recent population mixture in {India}},
	volume = {93},
	OPTissn = {0002-9297},
	OPTurl = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3769933/},
	OPTdoi = {10.1016/j.ajhg.2013.07.006},
	abstract = {Most Indian groups descend from a mixture of two genetically divergent populations: Ancestral North Indians ({ANI}) related to Central Asians, Middle Easterners, Caucasians, and Europeans; and Ancestral South Indians ({ASI}) not closely related to groups outside the subcontinent. The date of mixture is unknown but has implications for understanding Indian history. We report genome-wide data from 73 groups from the Indian subcontinent and analyze linkage disequilibrium to estimate {ANI}-{ASI} mixture dates ranging from about 1,900 to 4,200 years ago. In a subset of groups, 100\% of the mixture is consistent with having occurred during this period. These results show that India experienced a demographic transformation several thousand years ago, from a region in which major population mixture was common to one in which mixture even between closely related groups became rare because of a shift to endogamy.},
	number = {3},
