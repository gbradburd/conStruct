  keywords = {critical_branching_process isolation_by_distance spatial_structure coalescent_theory spatial_coalescent},
  number = 967,
  pages = {359--368},
  publisher = {The University of Chicago Press for The American Society of Naturalists},
  timestamp = {2010-03-22T06:25:53.000+0100},
  title = {A Pain in the Torus: Some Difficulties with Models of Isolation by Distance},
